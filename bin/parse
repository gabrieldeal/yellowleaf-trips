#!/usr/bin/perl
#-*-perl-*-

use strict;
use lib './perl/lib';

use Carp ();
use Data::Dumper ();
use Scramble::Build ();

local $^W = 1;
local $| = 1;
local $SIG{__WARN__} = sub { Carp::confess "DIEING on warning: @_" };
local $SIG{__DIE__}  = sub { Carp::confess "DIEING: @_" };
$ENV{NODE_ENV} ||= 'production';
local $Data::Dumper::Maxdepth = 5;

exit(Scramble::Build::create());
