#!/usr/bin/env bash
umask 022
debug="-d:NYTProf"

output_dir="/vagrant_gabriel_home/projects/yellowleaf-trips/"
dir="$(dirname "$0")/.."
cd "$dir" \
    && perl bin/parse \
	    --dictionary-dir dict \
	    --data-directory "$output_dir/data/" \
	    --images-directory "$output_dir/data/gabrielx/reports/" \
	    --out "$output_dir/html" \
	    -v \
	    "$@"
